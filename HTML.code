<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Calculator ðŸ–© </title>
    <link rel="stylesheet" href="style.css">
</head>
<body>
    <div id="display">
        <input type="text" placeholder="0" readonly>
           
     <div>
        <button onclick="clearDisplay('')" class="one">AC</button>
        <button class="one">DE</button>
        <button onclick="appendToDisplay('.')" class="one">.</button>
        <button onclick="appendToDisplay('/')" class="one">/</button>
    </div>
    <div>
        <button onclick="appendToDisplay('7')">7</button>
        <button onclick="appendToDisplay('8')">8</button>
        <button onclick="appendToDisplay('9')">9</button>
        <button onclick="appendToDisplay('*')" class="one">*</button>
    </div>
    <div>
        <button onclick="appendToDisplay('4')">4</button>
        <button onclick="appendToDisplay('5')">5</button>
        <button onclick="appendToDisplay('6')">6</button>
        <button onclick="appendToDisplay('-')" class="one">-</button>
    </div>
    <div>
        <button onclick="appendToDisplay('1')">1</button>
        <button onclick="appendToDisplay('2')">2</button>
        <button onclick="appendToDisplay('3')">3</button>
        <button onclick="appendToDisplay('+')" class="one">+</button>
    </div>
    <div>
        <button onclick="appendToDisplay('00')">00</button>
        <button onclick="appendToDisplay('0')">0</button>
        <button style="width: 140px;" onclick="calculate('')" class="one">=</button>
        
    </div>
    </div>
    <script src="index.js"></script>

</body>
</html>
